{
	"manifest_version": 3,
	"name": "avanza-night-mode",
	"description": "Avanza Night Mode",
	"version": "1.1.0",
	"icons": {
		"16": "./src/icons/icon-16.png",
		"32": "./src/icons/icon-48.png",
		"48": "./src/icons/icon-48.png",
		"128": "./src/icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.avanza.se/*"
			],
			"css": [
				"src/css/avanza-night-mode.css"
			]
		}
	]
}
